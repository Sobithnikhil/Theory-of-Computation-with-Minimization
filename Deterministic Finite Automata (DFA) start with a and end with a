#include <stdio.h>
#include <string.h>
#define MAX_LEN 100
int main() 
{
  char input_string[MAX_LEN];
  int i;
  int state = 0;
  printf("Enter a string: ");
  scanf("%s", input_string);
  for (i = 0; i < strlen(input_string); i++) 
  {
    switch (state) 
	{
      case 0:
        if (input_string[i] == 'a') 
		{
          state = 1;
        } else 
		{
          state = 2;
        }
        break;
      case 1:
        state = 1;
        break;
      case 2:
        break;
    }
  }
  if (state == 1 && input_string[strlen(input_string) - 1] == 'a') 
  {
    printf("Accepted\n");
  } else {
    printf("Rejected\n");
  }
  return 0;
}
